<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assist.assistteachme">

    //permission to the internet
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MyAccountMenu" />
        <activity android:name=".BrowseThroughCourses">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginScreenActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".FinishCourseScreenActivity" />
        <activity
            android:name=".MainViewActivity"
            android:label="@string/title_activity_drawer_test"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".DrawerTestActivity"
            android:label="@string/title_activity_browse_through_courses"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>